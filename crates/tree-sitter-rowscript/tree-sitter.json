{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rowscript",
      "camelcase": "RowScript",
      "title": "RowScript",
      "scope": "source.rowscript",
      "file-types": [
        "rowscript"
      ],
      "injection-regex": "^rowscript$",
      "class-name": "TreeSitterRowscript"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for RowScript",
    "authors": [
      {
        "name": "RowScript developers",
        "email": "anqurvanillapy@gmail.com",
        "url": "https://rows.ro/"
      }
    ],
    "links": {
      "repository": "https://github.com/rowscript/rowscript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}