type RepKind =
    [ RepKindNumber
    | RepKindString
    | RepKindBoolean
    | RepKindBigint
    | RepKindUnit
    | RepKindObject
    | RepKindEnum
    ];

interface Reflector {
    reflect(t: Reflector): Reflected<Reflector>;
}

function unreflect<'R, T>(r: {'R}): T
where (value: T) keyof 'R
{
    r.value
}
