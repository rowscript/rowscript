import .outside;

class N {
    n: number;
}

const n0 = new N(42);
testing.assert(n0.n == 42);

function foo(): string {
    "foo"
}

class Foo {
    n: number;
    m: string = "hello";

    update() {
        this.n = this.n + 1;
    }

    foo(): string {
        this.update();
        "Foo::foo"
    }
}

const s0 = new Foo(42).foo();
testing.assert(s0 == "Foo::foo");

const s1 = new ::outside::Outside("hello").outside();
testing.assert(s1 == "outside");
