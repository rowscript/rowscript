function f0<Args>(s: string, ...: Args): Args;
function f1<Args>(...: Args): Args;

function t0() {
    const (a0, b0) = f0("t0", 42, true);
    console.log("a0", a0);
    console.log("b0", b0);
}
t0();

function t1() {
    if (true) {
        const (a1, b1) = f1("t1", 42, true);
