function log(msg: string);

class A {
    name: string;
    init { log(this.name) }
    hello() { log("hi") }
}

function f0() {
    let a = new A("John");
    a:hello()
}

class B<T> {
    name: T;
    init { log("hello") }
    hello() { log("hi") }
}

function f1() {
    let a = new B("John");
    a:hello()
}

f0();
f1();
