type Option<T> = [Ok: T | None];

function isOk<'A, T>(o: ['A]): boolean
where (Ok: T) <: 'A
{
    switch (o) {
    case Ok(v): true
    case v: false
    }
}

const o0: Option<number> = Ok(42);
const a0 = o0.isOk();
const v0 = o0.unwrap();

const o1: Option<number> = None;
const a1 = o1.isOk();
const v1 = o1.unwrap();
