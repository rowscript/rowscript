interface ToLowerCase<T> {
    toLowerCase(a: T): string {
        switch (typeof a) {
            case TypeofEnum:
                (keyof a).map((k) => k.toString().toLowerCase()).join("")
            case x:
                a.toLowerCase()
        }
    }
}

instanceof ToLowerCase<string> {
    toLowerCase(a: string): string {
        string#toLowerCase(a)
    }
}
